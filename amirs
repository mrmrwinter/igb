import os   #imports os


SAMPLE, = glob_wildcards("data/input/{sample}.cds_nt.fa")

rule all_outputs:
    input:
        expand("outputs/{sample}/igbpyOut.png", sample=SAMPLE)

rule igb:
    input:
        "data/input/{sample}.cds_nt.fa"
    output:
        "outputs/{sample}/igbpyOut.png"
    params:
        "{sample}"
    script:
      "igbsm.py"
